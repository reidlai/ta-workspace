{
  "name": "appshell-workspace",
  "private": true,
  "workspaces": [
    "apps/*",
    "modules/demo/*",
    "modules/core/ts/*",
    "scripts/*"
  ],
  "os": ["!win32"],
  "scripts": {
    "preinstall": "./scripts/check-platform.sh",
    "moon": "moon",
    "setup": "./scripts/check-platform.sh && moon setup",
    "ci": "moon ci",
    "check-platform": "./scripts/check-platform.sh"
  },
  "pnpm": {
    "overrides": {
      "cookie": "^0.7.0"
    },
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  },
  "engines": {
    "node": "~20"
  },
  "devDependencies": {
    "@moonrepo/cli": "^1.41.7"
  },
  "packageManager": "pnpm@10.26.2"
}
